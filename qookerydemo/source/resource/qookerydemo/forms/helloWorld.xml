<?xml version="1.0" encoding="utf-8"?>
<!--
	A "Hello World" example for Qookery. It presents
	a button to the user which opens an alert when clicked.
-->
<form xmlns="http://www.qookery.org/ns/Form">

	<button label="Click me for a surprise">
		<script event="execute">
			alert("Hello, World!");
		</script>
	</button>
	
</form>
