{
	"name": "qookerydemo",
	"include": [
		{ "path" : "${QOOXDOO_PATH}/tool/data/config/application.json" }
	],
	"export": [
		"build",
		"clean",
		"lint",
		"source"
	],
	"default-job": "source",
	"let": {
		"ROOT": ".",
		"APPLICATION": "qookerydemo",
		"QXTHEME": "qookerydemo.thene.Theme",
		"LOCALES": [ "en" ],
		"CACHE": "${TMPDIR}/qx${QOOXDOO_VERSION}/cache"
	},
	"jobs": {
		"libraries": {
			"library": [
				{ "manifest" : "../qookery/Manifest.json" }
			]
		},
		"includes": {
			"include": [
				"${APPLICATION_MAIN_CLASS}",
				"${QXTHEME}",
				"qookery.*",
				"qookerydemo.*"
			]
		}
	}
}
