<fileset-config file-format-version="1.2.0" simple-config="true" sync-formatter="false">
	<local-check-config name="Qookery" location="/org.qooxdoo/.settings/checkstyle/configuration.xml" type="project" />
	<fileset name="all" enabled="true" check-config-name="Qookery" local="false">
		<file-match-pattern match-pattern="." include-pattern="true" />
		<file-match-pattern match-pattern="qookerydemo/source/resource/qookerydemo/forms" include-pattern="false" />
	</fileset>
	<filter name="NonSrcDirs" enabled="false" />
</fileset-config>
